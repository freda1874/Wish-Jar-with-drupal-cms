{%
  set classes = [
    'region',
    'region-' ~ region|clean_class,
    'flex',
    'flex-row',
   'justify-evenly',
    'text-sm',
    'px-20',
    'bg-black',
    'font-post',
    'text-zinc-100',
  ]
%}
{% if content %}
	<div{{attributes.addClass(classes)}}>
		{{ content }}
	</div>
{% endif %}
